    filter_content_types = 0
    global_allow = 1
    #content_icon = 'NursingSignupSheet.gif'
    immediate_view = 'base_view'
    default_view = 'base_view'
    suppl_views = ()
    typeDescription = "Nursing Appointment Signup Sheet"
    typeDescMsgId = 'description_edit_nursingsignupsheet'

    _at_rename_after_creation = True

    schema = NursingSignupSheet_schema

